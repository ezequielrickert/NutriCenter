+package org.example.controller;
+
+import org.example.model.SuperAdmin;
+
+import java.security.SecureRandom;
+import java.util.Base64;
+import java.util.HashMap;
+import java.util.Map;
+import java.util.Objects;
+
+public class LoginController{
+
+    static Map<String, String> usernameTokenMap = new HashMap<>();
+    private static final SecureRandom secureRandom = new SecureRandom(); //threadsafe
+    private static final Base64.Encoder base64Encoder = Base64.getUrlEncoder(); //threadsafe
+
+    public boolean verifyLogin(String username, String token) {
+        String savedToken = usernameTokenMap.get(username);
+        return Objects.equals(savedToken, token);
+    }
+
+
+    public static String generateNewToken() {
+        byte[] randomBytes = new byte[24];
+        secureRandom.nextBytes(randomBytes);
+        return base64Encoder.encodeToString(randomBytes);
+    }
+
+}
